{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/graphql",
        "dest": "src/graphql/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "src/index.ts"
      }
    ]
  }
  